<h1>Listing recipe_ingredience_links</h1>

<table>
  <tr>
    <th>Recipe</th>
    <th>Ingredience</th>
    <th>Importance</th>
    <th>Quantity</th>
    <th>Note</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @recipe_ingredience_links.each do |recipe_ingredience_link| %>
  <tr>
    <td><%= recipe_ingredience_link.recipe %></td>
    <td><%= recipe_ingredience_link.ingredience %></td>
    <td><%= recipe_ingredience_link.importance %></td>
    <td><%= recipe_ingredience_link.quantity %></td>
    <td><%= recipe_ingredience_link.note %></td>
    <td><%= link_to 'Show', recipe_ingredience_link %></td>
    <td><%= link_to 'Edit', edit_recipe_ingredience_link_path(recipe_ingredience_link) %></td>
    <td><%= link_to 'Destroy', recipe_ingredience_link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Recipe ingredience link', new_recipe_ingredience_link_path %>
